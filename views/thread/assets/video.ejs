<%
const url = {
    original: post.file.meta.originalSrc,
    fallback: post.file.meta.fullSrc
};
const autoplay = renderOpts.autoplay ? ' autoplay' : '';
const loop = renderOpts.loop ? ' loop' : '';
const volume = renderOpts.volume / 100;
-%>
<video controls <%- autoplay + loop -%> onloadstart="this.volume=<%- volume -%>" preload="none" poster="<%- post.file.meta.thumbSrc -%>" data-poster="<%- post.file.meta.thumbFullSrc -%>">
    <source src="<%- url.original -%>">
    <source src="<%- url.fallback -%>">
</video>
