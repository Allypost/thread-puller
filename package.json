{
  "name": "thread-puller",
  "version": "1.0.0",
  "description": "ThreadPuller - View 4Chan thread images and videos",
  "author": "Allypost",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev": "cross-env NODE_ENV=development nodemon --exec \"clear && node\" server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@bazzite/nuxt-optimized-images": "^0.2.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/router": "^1.4.0",
    "axios": "latest",
    "consola": "latest",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "fluent-ffmpeg": "^2.1.2",
    "html-entities": "latest",
    "ioredis": "^4.14.0",
    "nuxt": "^2.0.0",
    "nuxt-svg-loader": "^1.0.1",
    "striptags": "^3.1.1",
    "vuex": "latest",
    "vue-router": "latest",
    "vue": "latest"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^0.2.1",
    "autoprefixer": "latest",
    "babel-eslint": "^10.0.1",
    "cssnano": "latest",
    "eslint": "^5.16.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^2.6.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "lint-staged": "^8.2.1",
    "mobile-detect": "^1.4.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "pixrem": "^5.0.0",
    "postcss-font-magician": "^2.2.2",
    "postcss-normalize": "^8.0.1",
    "sass-loader": "^7.2.0"
  },
  "browserslist": [
    "defaults"
  ]
}
