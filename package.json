{
  "name": "thread-puller",
  "version": "1.0.0",
  "description": "ThreadPuller - View 4Chan thread images and videos",
  "author": "Allypost",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev": "cross-env NODE_ENV=development nodemon --exec \"clear && node\" server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@bazzite/nuxt-optimized-images": "^0.2.1",
    "@nuxtjs/axios": "^5.8.0",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/router": "^1.5.0",
    "axios": "latest",
    "body-parser": "^1.19.0",
    "consola": "latest",
    "cookie-universal-nuxt": "^2.0.19",
    "cross-env": "latest",
    "express": "^4.16.4",
    "fluent-ffmpeg": "^2.1.2",
    "fuse.js": "^3.4.6",
    "html-entities": "latest",
    "ioredis": "^4.14.1",
    "nuxt": "^2.10.2",
    "nuxt-svg-loader": "^1.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "striptags": "^3.1.1",
    "sweet-modal-vue": "^2.0.0",
    "uuid": "^3.3.3",
    "vue": "latest",
    "vue-router": "^3.1.3",
    "vuex": "latest",
    "vuex-shared-mutations": "^1.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "autoprefixer": "latest",
    "babel-eslint": "^10.0.3",
    "cssnano": "latest",
    "eslint": "^6.7.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^3.1.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "lint-staged": "^9.4.3",
    "mobile-detect": "^1.4.4",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "pixrem": "^5.0.0",
    "postcss-font-magician": "^2.3.1",
    "postcss-normalize": "^8.0.1",
